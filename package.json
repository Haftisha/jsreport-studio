{
  "name": "react-redux-universal-hot-example",
  "description": "Example of an isomorphic (universal) webapp using react redux and hot reloading",
  "author": "Erik Rasmussen <rasmussenerik@gmail.com> (http://github.com/erikras)",
  "license": "MIT",
  "version": "0.9.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/erikras/react-redux-universal-hot-example"
  },
  "homepage": "https://github.com/erikras/react-redux-universal-hot-example",
  "keywords": [
    "react",
    "isomorphic",
    "universal",
    "webpack",
    "express",
    "hot reloading",
    "react-hot-reloader",
    "redux",
    "starter",
    "boilerplate",
    "babel"
  ],
  "main": "src/server.js",
  "standard": {
    "parser": "babel-eslint"
  },
  "scripts": {
    "test": "mocha test --compilers js:babel-register --recursive",
    "build": "set NODE_ENV=production&&webpack --verbose --colors --display-error-details --config webpack/prod.config.js"
  },
  "betterScripts": {
    "start-prod": {
      "command": "node ./src/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "production",
        "PORT": 8080,
        "APIPORT": 3030
      }
    },
    "start-dev": {
      "command": "node ./src/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "PORT": 3000,
        "APIPORT": 3030
      }
    },
    "watch-client": {
      "command": "node webpack/webpack-dev-server.js",
      "env": {
        "UV_THREADPOOL_SIZE": 100,
        "NODE_PATH": "./src",
        "PORT": 3000,
        "APIPORT": 3030
      }
    },
    "build": {
      "command": "webpack --verbose --colors --display-error-details --config webpack/prod.config.js",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "dependencies": {
    "babel-core": "^6.5.2",
    "babel-loader": "^6.2.1",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-react-display-name": "^6.3.13",
    "babel-plugin-transform-runtime": "^6.6.0",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "babel-register": "^6.3.13",
    "babel-runtime": "^6.3.19",
    "bluebird": "^3.3.4",
    "brace": "^0.8.0",
    "compression": "^1.6.0",
    "express": "^4.13.3",
    "file-loader": "^0.8.5",
    "jsreport": "^0.13.2",
    "less": "^2.6.1",
    "lodash": "^4.10.0",
    "postcss-loader": "^0.8.2",
    "pretty-error": "^2.0.0",
    "react": "^15.0.1",
    "react-ace": "^3.2.0",
    "react-dom": "^15.0.1",
    "react-helmet": "^3.0.1",
    "react-inline-css": "^2.0.0",
    "react-list": "^0.7.16",
    "react-redux": "^4.0.0",
    "react-router": "^2.0.0",
    "react-router-redux": "^4.0.2",
    "react-split-pane": "^0.1.25",
    "react-transform-hmr": "^1.0.4",
    "redux": "^3.0.4",
    "redux-batched-actions": "^0.1.2",
    "redux-thunk": "^2.0.1",
    "resolve-url-loader": "^1.4.3",
    "serve-favicon": "^2.3.0",
    "superagent": "^1.4.0",
    "url-loader": "^0.5.7"
  },
  "devDependencies": {
    "babel-eslint": "^6.0.0-beta.6",
    "babel-plugin-react-transform": "^2.0.0",
    "babel-plugin-typecheck": "^3.6.0",
    "better-npm-run": "0.0.8",
    "clean-webpack-plugin": "^0.1.6",
    "css-loader": "^0.23.1",
    "eslint": "^2.7.0",
    "eslint-plugin-babel": "^3.2.0",
    "eslint-plugin-react": "^4.2.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "font-awesome": "^4.4.0",
    "font-awesome-webpack": "0.0.4",
    "json-loader": "^0.5.4",
    "mocha": "^2.4.5",
    "node-sass": "^3.4.2",
    "react-transform-catch-errors": "^1.0.0",
    "react-transform-hmr": "^1.0.1",
    "redbox-react": "^1.1.1",
    "redux-immutable-state-invariant": "^1.2.2",
    "redux-logger": "^2.6.1",
